{
    "breadcrumbs.enabled": false,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true,
    },
    "editor.fontFamily": "FiraCode-Retina",
    "editor.fontLigatures": true,
    "editor.fontSize": 14,
    "editor.minimap.enabled": false,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.renderWhitespace": "none",
    "editor.tabSize": 2,
    "explorer.confirmDelete": false,
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "python.formatting.provider": "black",
    "workbench.activityBar.visible": false,
    "workbench.colorCustomizations": {
        "[Default Dark+]": {
            "statusBar.background": "#2D2D2D",
            "statusBar.noFolderBackground": "#2D2D2D",
        },
        "terminal.background":"#282a36",
        "terminal.foreground":"#f8f8f2",
        "terminalCursor.foreground":"#6272a4",
        "terminalCursor.background":"#f8f8f2",
        "terminal.ansiBlack":"#44475a",
        "terminal.ansiBlue":"#bd93f9",
        "terminal.ansiBrightBlack":"#6272a4",
        "terminal.ansiBrightBlue":"#bd93f9",
        "terminal.ansiBrightCyan":"#8be9fd",
        "terminal.ansiBrightGreen":"#50fa7b",
        "terminal.ansiBrightMagenta":"#ff79c6",
        "terminal.ansiBrightRed":"#ff5555",
        "terminal.ansiBrightWhite":"#feffff",
        "terminal.ansiBrightYellow":"#f1fa8c",
        "terminal.ansiCyan":"#8be9fd",
        "terminal.ansiGreen":"#50fa7b",
        "terminal.ansiMagenta":"#ff79c6",
        "terminal.ansiRed":"#ff5555",
        "terminal.ansiWhite":"#f8f8f2",
        "terminal.ansiYellow":"#f1fa8c"
    },
    "workbench.startupEditor": "newUntitledFile",
    "workbench.statusBar.visible": true,
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact",
    ],
    "window.zoomLevel": 0,
    "terminal.integrated.fontFamily": "FiraCode-Retina",
    "terminal.integrated.fontSize": 14,
    "terminal.integrated.fontWeight": "400",
    "terminal.integrated.fontWeightBold": "400",
    "terminal.integrated.lineHeight": 1.1,
}
