        // "terminal.ansiBrightRed": "#d16969",
        // "terminal.ansiBrightGreen": "#6A9955",
        // "terminal.ansiBrightYellow": "#d7ba7d",
        // "terminal.ansiBrightBlue": "#569CD6",
        // "terminal.ansiBrightMagenta": "#C586C0",
        // "terminal.ansiBrightCyan": "#51B6C4",
        // "terminal.ansiBrightWhite": "#D4D4D4",
    },
    "workbench.startupEditor": "newUntitledFile",
    "workbench.statusBar.visible": true,
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact",
    ],
    "window.zoomLevel": 0,
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.fontFamily": "FiraCode-Retina",
    "terminal.integrated.fontSize": 15,
    "terminal.integrated.fontWeight": "400",
    "terminal.integrated.fontWeightBold": "400",
    "javascript.updateImportsOnFileMove.enabled": "never",
    "editor.suggestSelection": "first",
    "editor.accessibilitySupport": "off",
    "[markdown]": {
        "editor.defaultFormatter": "tehnix.vscode-tidymarkdown"
    }
}
