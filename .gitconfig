[user]
  name = Cabe Branson
  email = cabebranson@gmail.com
[alias]
  st = status
  ci = commit
  br = branch
  co = checkout
  ls = ls-files
  df = diff
  rs = reset
  undo = reset --soft HEAD~
  fucked = reset --hard HEAD~
[core]
  excludesfile = ~/.gitignore
[color]
  ui = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[push]
  default = simple
[filter "lfs"]
  clean = git-lfs clean %f
  smudge = git-lfs smudge %f
  required = true
[credential]
  helper = osxkeychain
[include]
  gitlab = .gitlab/.gitconfig
